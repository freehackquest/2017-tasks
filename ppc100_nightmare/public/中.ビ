def 六(李):
    再 = '241o0'[::月]









    没 = list(李)

    美 = ''






    for 妹 in 没:












        名 = ord(妹)










































































































        美 += str(bin(名))[::月].replace(chr(int(再, int('1000',int(张)+2))), 张).replace(张,'.').replace(str(月 + (int(张)+2)),'-') + ' '






    return 美


































张 = '0'










月 = int(张) - 1
































是 = ''




师 = 六(是)